{
    "compilerOptions": {
      "target": "ES2020",
      "module": "ESNext",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "outDir": "netlify/functions-dist",
      "strict": true,
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "declaration": false,
      "sourceMap": false,
      "noEmit": false
    },
    "include": ["netlify/functions/**/*.ts"],
    "exclude": ["node_modules", "netlify/functions-dist"]
  }
  
  